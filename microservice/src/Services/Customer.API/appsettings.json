{
  "DatabaseSettings": {
    "DBProvider": "postgresql",
    //"ConnectionString": "Server=localhost;Port=5433;Database=CustomerDb;Uid=admin;Pwd=admin1234;",
    "ConnectionString": "Server=localhost;Port=5432;Database=CustomerDb;Uid=postgres;Pwd=postgres;"
  },

  "HangfireSettings": {
    "Route": "/jobs",
    "ServerName": "Customer API",
    "Dashboard": {
      "AppPath": "/",
      "StatsPollingInterval": 2000,
      "DashboardTitle": "Jobs"
    },
    "Storage": {
      "DBProvider": "postgresql",
      //"ConnectionString": "Server=localhost;Port=5433;Database=CustomerDb;Uid=admin;Pwd=admin1234;",
      "ConnectionString": "Server=localhost;Port=5432;Database=CustomerDb;Uid=postgres;Pwd=postgres;"
    }
  },

  "SeriLog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore.Authentication": "Debug",
        "System": "Warning"
      }
    }
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200",
    "UserName": "elastic",
    "Password": "admin"
  },
  "AllowedHosts": "*"
}
